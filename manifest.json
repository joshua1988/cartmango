{
  "manifest_version": 2,

  "name": "Cart Mango",
  "description": "This extension shows you the future of online shopping",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/captainpangyo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/jquery.js","js/content_script.js"]
    }
  ],
  "permissions": ["tabs", "<all_urls>", "storage"]
}
